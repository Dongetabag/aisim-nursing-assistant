{
  "name": "aisim-nursing-assistant",
  "version": "1.0.0",
  "description": "AI-powered nursing charting assistant using Google Gemini API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:production": "NODE_ENV=production node server.production.js",
    "dev": "nodemon server.js",
    "test": "node test-system.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop aisim-nursing-production",
    "pm2:restart": "pm2 restart aisim-nursing-production",
    "pm2:logs": "pm2 logs aisim-nursing-production",
    "pm2:monit": "pm2 monit",
    "logs:clear": "rm -rf logs/*.log",
    "setup:production": "npm install --production && mkdir -p logs",
    "health": "curl -s http://localhost:3000/api/health | json_pp"
  },
  "keywords": [
    "nursing",
    "charting",
    "ai",
    "gemini",
    "healthcare",
    "automation"
  ],
  "author": "AISim",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
